{
  "name": "preload-polyfill",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "chokidar-cmd -c \"npm-run-parallel build:polyfill build:invoker\" -t *.js",
    "cleanup": "rm -rf dist && mkdir dist",
    "cs": "prettier --write *.js",
    "build:polyfill": "BABEL_ENV=production babel polyfill.js -o dist/polyfill.js",
    "build:invoker": "BABEL_ENV=production babel invoke.js -o dist/invoke.js",
    "build": "npm run cleanup && npm run cs && npm-run-parallel build:polyfill build:invoker"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.31",
    "@babel/core": "^7.0.0-beta.31",
    "@babel/preset-env": "^7.0.0-beta.31",
    "chokidar-cmd": "^1.2.1",
    "npm-run-parallel": "^0.6.0",
    "prettier": "^1.8.1"
  }
}
